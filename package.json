    "@types/umami": "^0.1.0",
    "commitlint": "^16.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "next-sitemap": "^2.1.5",
    "prettier": "^2.5.1",
