    "cors": "^2.8.5",
    "framer-motion": "^5.6.0",
    "gray-matter": "^4.0.3",
    "next": "^12.0.9",
    "next-seo": "^5.0.0",
    "preact": "^10.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-raw": "^6.1.1",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
