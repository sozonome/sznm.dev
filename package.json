{
  "name": "sznm.dev",
  "private": true,
  "description": "Agustinus Nathaniel's dev site",
  "version": "1.8.0",
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true yarn build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
